journal = {Journal of the American Society for Information Science},
keywords = {DECISION making,DECISION support systems,ELECTRONIC information resource literacy,INFORMATION measurement,INFORMATION science,INFORMATION technology},
number = {3},
pages = {221--231},
title = {{A Generalized Conceptual Development for the Analysis and Flow of Information.}},
url = {http://search.ebscohost.com/login.aspx?direct=true{\&}db=bth{\&}AN=18389403{\&}site=ehost-live},
volume = {24},
year = {1973}
}
@article{MacKenzie2003,
