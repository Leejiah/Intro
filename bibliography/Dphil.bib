journal = {South African Journal of Wildlife Research},
keywords = {bite size,cropping rate,foraging efficiency,functional-response,impala,intake rate,wapiti,zebra},
number = {2},
pages = {93--100},
title = {{Forage intake rates and foraging efficiency of free-ranging zebra and impala}},
volume = {32},
year = {2002}
}
