journal = {Biological Conservation},
keywords = {Carnivore conservation,Fences,Game ranching,Lycaon pictus},
pages = {113--121},
pmid = {2767},
title = {{Attitudes of ranchers towards African wild dogs Lycaon pictus: Conservation implications on private land}},
volume = {125},
year = {2005}
}
