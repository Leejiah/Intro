doi = {10.1016/j.biocon.2005.11.024},
file = {:Users/leejiahdorward/Documents/Mendeley Desktop/2006/Wang, Macdonald - 2006 - Livestock predation by carnivores in Jigme Singye Wangchuck National Park, Bhutan.pdf:pdf},
