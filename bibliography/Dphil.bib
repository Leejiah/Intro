doi = {10.1006/anbe.1996.0242},
file = {:Users/leejiahdorward/Documents/Mendeley Desktop/1996/Hadj-Chikh, Steele, Smallwood - 1996 - Caching decisions by grey squirrels a test of the handling time and perishability hypotheses.pdf:pdf},
