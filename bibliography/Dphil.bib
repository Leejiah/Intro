@article{Harihar2013,
abstract = {Carter et al. (1) argued that coexistence is practical at fine spatial scales based on temporal segregation between tigers and people in and around Chitwan National Park, Nepal. The authors presented their coexistence model as an alternative to the widely held principle that long-term tiger conservation requires extensive areas where human-use is excluded. We believe that this study makes a fundamental logic error by conflating "co-occurrence" with "coexistence." Their data demonstrate spatial co-occurrence conditioned on temporal separation, an unsurprising finding. Coexistence, the relevant interspecific dynamic process, has not been demonstrated and requires long-term data they did not collect. The conclusion of human-tiger coexistence is based on three main research findings. First, similar tiger densities occur "inside" and "outside" Chitwan National Park. Second, the occurrence of tigers at camera locations is unrelated to human use. Third, the co-occurrence of tigers and humans at camera locations are temporally separated.},
author = {Harihar, Abishek and Chanchani, Pranav and Sharma, Rishi Kumar and Vattakaven, Joseph and Gubbi, Sanjay and Pandav, Bivash and Noon, Barry},
doi = {10.1073/pnas.1217001110},
file = {:Users/leejiahdorward/Documents/Mendeley Desktop/2013/Harihar et al. - 2013 - Conflating co-occurrence with coexistence.pdf:pdf},
isbn = {1091-6490 (Electronic)$\backslash$r0027-8424 (Linking)},
issn = {1091-6490},
journal = {Proceedings of the National Academy of Sciences of the United States of America},
keywords = {Animals,Conservation of Natural Resources,Conservation of Natural Resources: methods,Ecology,Ecology: methods,Ecosystem,Humans,Tigers,Tigers: physiology},
number = {2},
pages = {E109},
pmid = {23284175},
title = {{Conflating "co-occurrence" with "coexistence".}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=3545792{\&}tool=pmcentrez{\&}rendertype=abstract},
volume = {110},
year = {2013}
}
