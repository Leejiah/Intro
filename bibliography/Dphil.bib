journal = {Journal of Applied Ecology},
keywords = {Cocos nucifera,Crop raiding,Procolobus kirkii,Pruning by monkeys},
number = {6},
pages = {1009--1020},
title = {{Colobus monkeys and coconuts: A study of perceived human-wildlife conflicts}},
volume = {36},
year = {1999}
}
