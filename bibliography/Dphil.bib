doi = {10.1111/cobi.12244},
file = {:Users/leejiahdorward/Documents/Mendeley Desktop/2014/Hazzah et al. - 2014 - Efficacy of two lion conservation programs in Maasailand, Kenya.pdf:pdf},
isbn = {1523-1739},
