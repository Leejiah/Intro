journal = {Conservation Biology},
keywords = {Carnivore,Community involvement,Community-based conservation,Compensation,Conservation,Conservation evaluation,Incentives,Local knowledge,Tolerance},
number = {3},
pages = {851--860},
pmid = {24527992},
title = {{Efficacy of two lion conservation programs in Maasailand, Kenya}},
volume = {28},
year = {2014}
