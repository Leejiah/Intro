file = {:Users/leejiahdorward/Documents/Mendeley Desktop/2007/Moser, Garton - 2007 - Effects of Telemetry Location Error on Space-Use Estimates Using a Fixed-Kernel Density Estimator.pdf:pdf},
isbn = {0022-541X},
issn = {0022-541X},
journal = {Journal of Wildlife Management},
keywords = {cross validation,error ratio,fixed kernel,home range,location error,radiotelemetry,space use,utilization},
number = {7},
pages = {2421--2426},
title = {{Effects of Telemetry Location Error on Space-Use Estimates Using a Fixed-Kernel Density Estimator}},
