file = {:Users/leejiahdorward/Documents/Mendeley Desktop/2013/Zarco-Gonz{\'{a}}lez, Monroy-Vilchis, Alan{\'{i}}z - 2013 - Spatial model of livestock predation by jaguar and puma in Mexico Conservation plannin.pdf:pdf},
isbn = {0006-3207},
issn = {00063207},
journal = {Biological Conservation},
keywords = {Ecological niche model,Jaguar,Livestock,M??xico,Predation,Puma},
pages = {80--87},
title = {{Spatial model of livestock predation by jaguar and puma in Mexico: Conservation planning}},
volume = {159},
year = {2013}
}
