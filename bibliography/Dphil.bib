journal = {Oryx},
keywords = {capra sibirica,doi,dx,for this article,http,local knowledge,lynx lynx,occu-,org,please visit,s,to view supplementary material},
number = {2},
pages = {1--11},
title = {{Status assessment of the Endangered snow leopard Panthera uncia and other large mammals in the Kyrgyz Alay, using community knowledge corrected for imperfect detection}},
url = {http://www.journals.cambridge.org/abstract{\_}S0030605315000502},
volume = {50},
