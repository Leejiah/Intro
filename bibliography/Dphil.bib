isbn = {1365-2664},
issn = {00218901},
journal = {Journal of Applied Ecology},
keywords = {Connochaetus taurinus,Lion density,Lion predation,Migratory and sedentary ungulates,Prey density,Wildlife cropping},
number = {3},
pages = {431--441},
title = {{Modelling wildebeest population dynamics: Implications of predation and harvesting in a closed system}},
volume = {42},
year = {2005}
}
