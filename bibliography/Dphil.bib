issn = {0962-8452},
journal = {Proceedings. Biological sciences / The Royal Society},
keywords = {beverton,first-principles derivation,hassell model,holt model,negative-binomial distribution,ricker model,skellam model},
number = {1576},
pages = {2065--2072},
pmid = {16191618},
title = {{The role of competition and clustering in population dynamics.}},
volume = {272},
year = {2005}
}
